{
  "version": 2,
  "routes": [
    {
      "src": "/(.*?)",
      "dest": "index.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH"],
      "headers": [
        {
          "source": "/(.*)",
          "headers": [
            { "key": "Access-Control-Allow-Origin", "value": "https://twittercloneinmern-fe.vercel.app" }
          ]
        },
        {
          "source": "/twittercloneinmern-fe.vercel.app/(.*)",
          "headers": [
            { "key": "Access-Control-Allow-Origin", "value": "https://twittercloneinmern-fe.vercel.app" }
          ]
        }
      ]
    }
  ]
}
